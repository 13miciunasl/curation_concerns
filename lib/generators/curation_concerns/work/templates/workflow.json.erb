{
  "work_types": [
    {
      "name": "<%= file_name %>",
         "actions": [{
           "name": "finalize_digitization",
           "from_states": [{"name": "pending"}],
           "transition_to": "metadata_review",
           "attributes": {
             "presentation_sequence": 1
           }
         }, {
           "name": "finalize_metadata",
           "from_states": [{"name":"metadata_review"}],
           "transition_to": "final_review",
           "attributes": {
           	"presentation_sequence": 2
           }
         }, {
           "name": "complete",
           "from_states": [{"name":"final_review"}],
           "transition_to": "complete",
           "attributes": {
             "presentation_sequence": 3
           }
         }, {
           "name": "takedown",
           "from_states": [{"name":"complete"}],
           "transition_to": "takedown"
         }, {
           "name": "restore",
           "from_states": [{"name":"takedown"}],
           "transition_to": "complete"
         }, {
           "name": "flag",
           "from_states": [{"name":"complete"}],
           "transition_to": "flag"
         }, {
           "name": "unflag",
           "from_states": [{"name":"flag"}],
           "transition_to": "complete"
        }]
     }
  ]
}
